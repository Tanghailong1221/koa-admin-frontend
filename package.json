    "build:staging": "vue-tsc -b && vite build --mode staging",
    "build:prod": "vue-tsc -b && vite build --mode production",
    "preview": "vite preview",
    "generate": "node scripts/generate.js",
    "g:page": "node scripts/generate.js page",
    "g:comp": "node scripts/generate.js component",
    "g:api": "node scripts/generate.js api",
    "g:store": "node scripts/generate.js store",
    "lint": "eslint .",
    "lint:style": "stylelint \"src/**/*.{vue,css,scss}\"",
    "lint:prettier": "prettier --check \"**/*.{js,ts,vue,json,md,css,scss}\"",
    "test": "vitest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@sentry/vite-plugin": "^4.6.1",
    "@sentry/vue": "^10.32.1",
