# 开发环境配置

# 应用标题
VITE_APP_TITLE=前端架构增强项目 - 开发环境

# API 基础路径
VITE_API_BASE_URL=http://localhost:3000/api

# 是否启用 Mock 数据
VITE_USE_MOCK=true

# 是否启用性能监控
VITE_ENABLE_PERFORMANCE_MONITOR=true

# 性能监控采样率 (0-1)
VITE_PERFORMANCE_SAMPLE_RATE=1.0

# 是否启用版本检测
VITE_ENABLE_VERSION_CHECK=false

# 版本检测间隔（毫秒）
VITE_VERSION_CHECK_INTERVAL=60000

# 是否启用 Sentry
VITE_ENABLE_SENTRY=false

# Sentry DSN
VITE_SENTRY_DSN=

# 是否启用控制台日志
VITE_ENABLE_CONSOLE=true

# 是否启用数据加密
VITE_ENABLE_ENCRYPTION=false

# 加密密钥（开发环境使用固定密钥）
VITE_ENCRYPTION_KEY=dev-secret-key-12345678

# 上传文件大小限制（MB）
VITE_UPLOAD_MAX_SIZE=10

# 上传文件类型限制
VITE_UPLOAD_ALLOWED_TYPES=image/*,video/*,.pdf,.doc,.docx,.xls,.xlsx

# CDN 地址
VITE_CDN_URL=

# 是否启用 CDN
VITE_ENABLE_CDN=false
